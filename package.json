{
  "name": "@mean-dao/mean-protocol",
  "version": "1.1.0",
  "description": "Typescript SDKs for the Mean Protocol",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mean-dao/mean-sdk.git"
  },
  "keywords": [
    "mean-dao",
    "mean-protocol"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mean-dao/mean-sdk/issues"
  },
  "homepage": "https://github.com/mean-dao/mean-sdk#readme",
  "files": [],
  "scripts": {
    "build": "tsc -b ./packages/**/tsconfig.json",
    "clean": "rm -R ./packages/**/lib",
    "clean:win": "rmdir /s packages/**/lib",
    "publish": "npm publish --access public",
    "install:ddca": "cd ../../../ && npm i -S @mean-dao/ddca",
    "install:hla": "cd ../../../ && npm i -S @mean-dao/hybrid-liquidity-ag",
    "install:ms": "cd ../../../ && npm i -S @mean-dao/money-streaming",
    "install:msp": "cd ../../../ && npm i -S @mean-dao/msp",
    "install": "run-p install:*"
  }
}
